<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

	<servlet>
		<servlet-name>labelWelcome</servlet-name>
		<servlet-class>com.gae.AddWelcomeServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>labelWelcome</servlet-name>
		<url-pattern>/addWelcome</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>welcome</servlet-name>
		<servlet-class>com.gae.WelcomeServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>welcome</servlet-name>
		<url-pattern>/welcome</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Trainings</servlet-name>
		<servlet-class>com.gae.TrainingsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Trainings</servlet-name>
		<url-pattern>/trainings</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Exercices</servlet-name>
		<servlet-class>com.gae.ExercicesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Exercices</servlet-name>
		<url-pattern>/exercices</url-pattern>
	</servlet-mapping>


	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

</web-app>